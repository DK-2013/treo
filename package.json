{
  "name": "treo",
  "description": "Consistent API to IndexedDB",
  "repository": "treojs/treo",
  "version": "1.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "indexeddb",
    "treo",
    "consistency",
    "offline"
  ],
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "build": "browserify -t babelify src/index.js -s treo -o dist/treo.js && uglifyjs dist/treo.js -m > dist/treo.min.js",
    "test": "eslint src/ test/ && browserify-test -t babelify && SAUCE_USERNAME=treo zuul --no-coverage --tunnel-host http://treojs.com -- test/*.js",
    "development": "browserify-test -t babelify -w"
  },
  "dependencies": {
    "component-emitter": "^1.2.0",
    "idb-batch": "^1.0.0",
    "idb-factory": "^1.0.0",
    "idb-range": "^3.1.3",
    "idb-request": "^3.1.0",
    "storage-emitter": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-test": "^2.1.2",
    "chai": "^3.4.1",
    "es6-promise": "^3.0.2",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^3.1.0",
    "idb-schema": "^3.2.1",
    "lodash.map": "^4.0.0",
    "polyfill-function-prototype-bind": "0.0.1",
    "regenerator": "^0.8.42",
    "treo-websql": "^3.0.0",
    "uglify-js": "^2.4.23",
    "zuul": "^3.9.0"
  }
}
