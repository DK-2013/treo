{
  "name": "treo",
  "description": "Human interface for IndexedDB",
  "repository": "git@github.com:treojs/treo.git",
  "version": "0.5.0",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "indexeddb",
    "treo",
    "offline"
  ],
  "scripts": {
    "test": "browserify-test",
    "start": "browserify-test -w",
    "build": "npm run build-treo && npm run build-treo-promise",
    "build-treo": "browserify lib -s treo -o dist/treo.js",
    "build-treo-promise": "browserify plugins/treo-promise -s treoPromise -o dist/treo-promise.js"
  },
  "dependencies": {
    "component-type": "^1.1.0",
    "idb-range": "^2.3.0",
    "idb-schema": "^1.0.0",
    "promise": "^6.1.0"
  },
  "devDependencies": {
    "after": "^0.8.1",
    "browserify": "^9.0.4",
    "browserify-test": "^1.1.0",
    "chai": "^2.1.1",
    "treo-websql": "^1.0.3"
  },
  "files": [
    "lib",
    "plugins"
  ]
}
