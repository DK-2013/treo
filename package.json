{
  "name": "treo",
  "description": "Human interface for IndexedDB",
  "repository": "git@github.com:treojs/treo.git",
  "version": "0.6.0-rc2",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "indexeddb",
    "treo",
    "offline"
  ],
  "scripts": {
    "test": "browserify-test",
    "development": "browserify-test -w",
    "build": "browserify lib/index.js -s treo -o dist/treo.js && uglifyjs dist/treo.js -m > dist/treo.min.js",
    "stat": "cloc --quiet --by-file lib/ && cloc --quiet --by-file --exclude-ext=json test/"
  },
  "dependencies": {
    "component-emitter": "^1.2.0",
    "idb-range": "^3.1.2",
    "idb-request": "^1.1.2",
    "idb-schema": "^3.1.1"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "es6-promise": "^3.0.2",
    "lodash.pluck": "^3.1.2",
    "treo-websql": "^2.0.0",
    "uglify-js": "^2.4.23",
    "babel-cli": "6.1.18",
    "babel-core": "6.1.18",
    "babel-eslint": "^4.1.5",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify-test": "^2.1.2",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "zuul": "^3.7.3"
  }
}
