{
  "name": "treo",
  "description": "Consistent API to IndexedDB",
  "repository": "treojs/treo",
  "version": "0.6.0-rc2",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "indexeddb",
    "treo",
    "consistency",
    "offline"
  ],
  "scripts": {
    "prepublish": "babel src --out-dir lib",
    "build": "browserify -t babelify src/index.js -s treo -o dist/treo.js && uglifyjs dist/treo.js -m > dist/treo.min.js",
    "test": "eslint src/ test/ packages/**/src/ && browserify-test -t babelify && SAUCE_USERNAME=treo zuul --no-coverage --tunnel-host http://treojs.com -- test/*.js",
    "development": "browserify-test -t babelify -w"
  },
  "dependencies": {
    "component-emitter": "^1.2.0",
    "idb-factory": "^1.0.0",
    "idb-range": "^3.1.2",
    "idb-request": "^3.0.0",
    "idb-schema": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.1.18",
    "babel-core": "6.1.18",
    "babel-eslint": "^4.1.5",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-test": "^2.1.2",
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "lodash.pluck": "^3.1.2",
    "uglify-js": "^2.4.23",
    "zuul": "^3.7.3"
  }
}
